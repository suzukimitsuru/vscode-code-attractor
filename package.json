{
  "name": "code-attractor",
  "displayName": "Code Attractor",
  "shortName": "Attractor",
  "description": "I wish to make a attracting foce diagram of the relationship between chords",
  "version": "0.0.6",
  "preview": true,
  "publisher": "suzukimitsrugunmaJapan",
  "keywords": [
    "software",
    "visualise",
    "attracting-foce",
    "vscode-extension"
  ],
  "categories": [
    "Visualization"
  ],
  "icon": "media/codeattractor-icon.png",
  "homepage": "https://github.com/suzukimitsuru/vscode-code-attractor",
  "repository": {
    "type": "git",
    "url": "https://github.com/suzukimitsuru/vscode-code-attractor.git"
  },
  "bugs": {
    "url": "https://github.com/suzukimitsuru/vscode-code-attractor/issues"
  },
  "main": "./dist/extension.js",
  "activationEvents": [
    "onWebviewPanel:codeattractor.sidebar"
  ],
  "contributes": {
    "commands": [
      {
        "command": "codeattractor.openEditor",
        "title": "Open Editor",
        "shortTitle": "open",
        "category": "Code Attractor",
        "icon": "media/codeattractor-icon.svg",
        "enablement": "true"
      },
      {
        "command": "codeattractor.showLogs",
        "title": "Show Logs",
        "shortTitle": "open",
        "category": "Code Attractor",
        "icon": "media/codeattractor-icon.svg",
        "enablement": "true"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "codeattractor.openEditor",
          "group": "navigation",
          "when": "view == codeattractor.sidebar || view == explorer"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "codeattractor.sidebar",
          "name": "Code Attractor"
        }
      ]
    }
  },
  "scripts": {
    "vscode:package": "npm run package && npx vsce package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "uninstall": "code --uninstall-extension suzukimitsuru.code-attractor",
    "media-build": "cd media && webpack --mode=production",
    "media-start": "cd media && webpack-cli serve --mode development"
  },
  "engines": {
    "vscode": "^1.81.0"
  },
  "devDependencies": {
    "@types/glob": ">=8.1.0",
    "@types/mocha": "10.0.9",
    "@types/node": ">=20.2.5",
    "@types/three": ">=0.162.0",
    "@types/vscode": ">=1.81.0",
    "@types/vscode-webview": ">=1.57.3",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@typescript-eslint/parser": "8.14.0",
    "@vscode/test-electron": ">=2.3.2",
    "eslint": "9.14.0",
    "glob": "11.0.0",
    "mocha": "11.0.0",
    "ts-loader": ">=9.4.3",
    "typescript": ">=5.1.3",
    "@vscode/vsce": "^3.2.1",
    "webpack": ">=5.85.0",
    "webpack-cli": ">=5.1.1"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.29.0",
    "braces": ">=3.0.3",
    "cannon-es": ">=0.20.0",
    "cannon-es-debugger": ">=1.0.0",
    "three": ">=0.162.0",
    "vscode-languageclient": ">=9.0.1",
    "xml2js": ">=0.6.2"
  }
}
